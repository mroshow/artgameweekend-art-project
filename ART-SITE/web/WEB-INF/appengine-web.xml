<?xml version="1.0" encoding="UTF-8"?>
<appengine-web-app xmlns="http://appengine.google.com/ns/1.0">
    <application>artags-site</application>
    <version>3</version>
    <system-properties>
        <property name="java.util.logging.config.file" value="WEB-INF/logging.properties"/>
    </system-properties>
    <static-files>
        <include path="favicon.ico" expiration="10d" />
        <include path="/**.png" expiration="10d" />
        <include path="/**.jpg" expiration="10d" />
        <include path="/**.jpeg" expiration="10d" />
        <include path="/**.swf" expiration="10d" />
        <include path="/**.css" expiration="10d" />
        <include path="/**.js" expiration="10d" />
        <include path="/**.xml" expiration="10d" />
    </static-files>

</appengine-web-app>
